// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Participant {
  id           String   @id @default(uuid())
  registeredAt DateTime @default(now())
  name         String
}

model Organizer {
  id           String @id @default(uuid())
  name         String
  contactEmail String
  link         String
}
