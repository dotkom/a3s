enum EventType {
    SOCIAL
    COMPANY_ACADEMIC_COURSE
    COMPANY_PRESENTATION
    OTHER
}

type Event {
    id: Int!
    startTime: Time!
    endTime: Time!
    location: String!
    title: String!
    shortDescription: String!
    longDescription: String!
    imageUrl: String
    organizer: EventOrganizer!
    companies: [Company!]!
    isAttendanceEvent: Boolean!
    attendance: Attendance
    isPaidEvent: Boolean!
    price: Int!
    eventRules: [String!]!
    eventType: EventType
}